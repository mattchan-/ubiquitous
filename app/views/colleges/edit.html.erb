<% provide(:title, "Edit college") %> 
<h1>Update college</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@college) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>
</div>